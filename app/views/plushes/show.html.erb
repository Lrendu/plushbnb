
<div class="container">
<% if @plush.photo.attached? %>
    <%= cl_image_tag @plush.photo.key, class:"card-img-top"  %>
  <% else %>
<img class="card-img-top" src="data:image/jpeg;base64,">
<% end %>
<h1>Voici <%= @plush.name %></h1>
<h2><%= @plush.description %></h2>
<h3><%= @plush.price %> €</h3>
<h4>Propriétaire : <%= @plush.owner.name %></h4>

<div>
  <h3>La peluche n'est hélas plus disponible aux dates suivantes</h3>
  <% @plush.rentals.each do |rental| %>
    <p><%= rental.date %> - <%= rental.tenant.name %></p>

  <% end %>

<div>
  <h2>Choisir les dates de location de <%= @plush.name %>:</h2>

<div class="container">
  <div class="form-wrapper">
    <h2>Book a plush</h2>
    <%= simple_form_for [@plush, @rental] do |f| %>
      <%= f.input :date, label: "Sélectionnez votre date:",
                         as: :string,
                         required: false,
                         input_html: { id: "date", class: "d-none"} %>
      <%= f.hidden_field :tenant_id, value: current_user.id %>
      <%= f.hidden_field :plush_id, value: @plush.id %>
      <%= f.button :submit, "Book", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

